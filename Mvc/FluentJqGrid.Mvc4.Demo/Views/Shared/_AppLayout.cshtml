<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>FluentJqGrid Demo: @ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css", 
                   "~/Content/themes/base/uiall", 
                   "~/Scripts/css/jqgrid", 
                   "~/Content/themes/jqgradientca/gca",
                   "~/Content/shcss")
    @RenderSection("styles", required: false)
    @Scripts.Render("~/Scripts/jquery", 
                    "~/Scripts/jqueryui", 
                    "~/Scripts/jqgrid", 
                    "~/Scripts/shjs", 
                    "~/Scripts/jqplugins")
    @RenderSection("scripts", required: false)
    
    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-30285050-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>
</head>
<body>
    <header id="header">
        <div id="header-wrapper">
            <div id="title">
                <h1>FluentJqGrid</h1>
                <h6>JqGrid Helper for Asp.Net Mvc</h6>
            </div>
        </div>

    </header>
    <div id="wrapper">
        <div id="nav">
            <div id="menucontainer">
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Download", "Index", "Home")</li>
                    <li>@Html.ActionLink("Demo", "JsonLoad", "SimpleLoad")</li>
                    <li>@Html.ActionLink("Documentation", "Index", "Home")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
            <br />
            <div id="footer">
            </div>
        </div>
    </div>
</body>
</html>
